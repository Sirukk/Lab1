using System;

public class Printer
{
    public void Print(string value)
    {
        Console.WriteLine($"Рядок: {value}");
    }

    public void Print(int value)
    {
        Console.WriteLine($"Цiле число: {value}");
    }

    public void Print(double value)
    {
        Console.WriteLine($"Число з плаваючою комою: {value}");
    }
}

class Program
{
    static void Main()
    {
        Printer printer = new Printer();

        printer.Print("Об'єктно-орiєнтоване програмування");
        printer.Print(100);
        printer.Print(1.125);
    }
}
