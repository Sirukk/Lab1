using System;

class Program
{
    static void Main()
    {
        Puppy puppy = new Puppy("Зевс");

        puppy.PrintName();
        puppy.Voice();
        puppy.Jump();
        puppy.Run();
        puppy.Bite();
    }
}

class Animal
{
    public string Name { get; set; }

    public Animal(string name)
    {
        Name = name;
    }
}

class Dog : Animal
{
    public Dog(string name) : base(name) { }

    public void Voice()
    {
        Console.WriteLine($"{Name} гавкати!");
    }

    public void Run()
    {
        Console.WriteLine($"{Name} бiгти!");
    }

    public void Bite()
    {
        Console.WriteLine($"{Name} кусати!");
    }
}

class Puppy : Dog
{
    public Puppy(string name) : base(name) { }

    public void PrintName()
    {
        Console.WriteLine($"Щеня {Name}");
    }

    public void Jump()
    {
        Console.WriteLine($"{Name} стрибати!");
    }
}
