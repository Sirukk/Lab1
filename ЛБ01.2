using System;
namespace Factorial
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine("Введiть число для обчислення факторiалу:");
            int a = int.Parse(Console.ReadLine());
            long factorial = 1;
            for (int i = 1; i <= a; i++)
            {
                factorial *= i;
            }
            Console.WriteLine($"Факторiал числа дорiвнює {factorial}");
            Console.ReadLine();
        }
    }
}
